{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "c:/cpp/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion": "10.0.22000.0",
            "compilerPath": "C:/codingInstallations/MinGW/bin/gcc.exe",
            "cStandard": "c17",
            "intelliSenseMode": "${default}",
            "cppStandard": "c++17",
            "mergeConfigurations": false,
            "browse": {
                "path": [
                    "c:/cpp/**",
                    "${workspaceFolder}"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}